{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ProductFootprint_with_Hoc_Extension",
  "description": "Data Type \"ProductFootprint\" of PACT Tech Spec Version 2 with Hoc as a DataModelExtension",
  "type": "object",
  "required": [
    "comment",
    "companyIds",
    "companyName",
    "created",
    "id",
    "pcf",
    "productCategoryCpc",
    "productDescription",
    "productIds",
    "productNameCompany",
    "specVersion",
    "status",
    "version"
  ],
  "properties": {
    "id": {
      "$ref": "#/definitions/PfId"
    },
    "specVersion": {
      "$ref": "#/definitions/VersionString"
    },
    "precedingPfIds": {
      "anyOf": [
        {
          "$ref": "#/definitions/NonEmptyPfIdVec"
        },
        {
          "type": "null"
        }
      ]
    },
    "version": {
      "$ref": "#/definitions/VersionInteger"
    },
    "created": {
      "type": "string",
      "format": "date-time"
    },
    "updated": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time"
    },
    "status": {
      "$ref": "#/definitions/PfStatus"
    },
    "statusComment": {
      "type": [
        "string",
        "null"
      ]
    },
    "validityPeriodStart": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time"
    },
    "validityPeriodEnd": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time"
    },
    "companyName": {
      "$ref": "#/definitions/NonEmptyString"
    },
    "companyIds": {
      "$ref": "#/definitions/CompanyIdSet"
    },
    "productDescription": {
      "type": "string"
    },
    "productIds": {
      "$ref": "#/definitions/ProductIdSet"
    },
    "productCategoryCpc": {
      "$ref": "#/definitions/NonEmptyString"
    },
    "productNameCompany": {
      "$ref": "#/definitions/NonEmptyString"
    },
    "comment": {
      "type": "string"
    },
    "pcf": {
      "$ref": "#/definitions/CarbonFootprint"
    },
    "extensions": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "$ref": "#/definitions/DataModelExtension"
      }
    }
  },
  "additionalProperties": false,
  "definitions": {
    "HOC": {
      "type": "object",
      "required": [
        "co2eIntensityTTW",
        "co2eIntensityThroughput",
        "co2eIntensityWTW",
        "energyCarriers",
        "hocId",
        "hubType",
        "isAccredited",
        "isVerified"
      ],
      "properties": {
        "hocId": {
          "type": "string"
        },
        "description": {
          "type": [
            "string",
            "null"
          ]
        },
        "isVerified": {
          "type": "boolean"
        },
        "isAccredited": {
          "type": "boolean"
        },
        "hubType": {
          "$ref": "#/definitions/HubType"
        },
        "temperatureControl": {
          "anyOf": [
            {
              "$ref": "#/definitions/TemperatureControl"
            },
            {
              "type": "null"
            }
          ]
        },
        "hubLocation": {
          "anyOf": [
            {
              "$ref": "#/definitions/Location"
            },
            {
              "type": "null"
            }
          ]
        },
        "inboundTransportMode": {
          "anyOf": [
            {
              "$ref": "#/definitions/TransportMode"
            },
            {
              "type": "null"
            }
          ]
        },
        "outboundTransportMode": {
          "anyOf": [
            {
              "$ref": "#/definitions/TransportMode"
            },
            {
              "type": "null"
            }
          ]
        },
        "packagingOrTrEqType": {
          "anyOf": [
            {
              "$ref": "#/definitions/PackagingOrTrEqType"
            },
            {
              "type": "null"
            }
          ]
        },
        "packagingOrTrEqAmount": {
          "type": [
            "integer",
            "null"
          ],
          "format": "uint",
          "minimum": 0.0
        },
        "energyCarriers": {
          "$ref": "#/definitions/NonEmptyVec_for_EnergyCarrier"
        },
        "co2eIntensityWTW": {
          "$ref": "#/definitions/Decimal"
        },
        "co2eIntensityTTW": {
          "$ref": "#/definitions/Decimal"
        },
        "co2eIntensityThroughput": {
          "$ref": "#/definitions/HocCo2eIntensityThroughput"
        }
      }
    },
    "HubType": {
      "type": "string",
      "enum": [
        "transshipment",
        "storageAndTransshipment",
        "warehouse",
        "liquidBulkTerminal",
        "maritimeContainerterminal"
      ]
    },
    "TemperatureControl": {
      "type": "string",
      "enum": [
        "ambient",
        "refrigerated",
        "mixed"
      ]
    },
    "Location": {
      "type": "object",
      "required": [
        "city",
        "country"
      ],
      "properties": {
        "street": {
          "type": [
            "string",
            "null"
          ]
        },
        "zip": {
          "type": [
            "string",
            "null"
          ]
        },
        "city": {
          "type": "string"
        },
        "country": {
          "$ref": "#/definitions/GeographicScope"
        },
        "iata": {
          "anyOf": [
            {
              "$ref": "#/definitions/IataCode"
            },
            {
              "type": "null"
            }
          ]
        },
        "locode": {
          "anyOf": [
            {
              "$ref": "#/definitions/Locode"
            },
            {
              "type": "null"
            }
          ]
        },
        "uic": {
          "anyOf": [
            {
              "$ref": "#/definitions/UicCode"
            },
            {
              "type": "null"
            }
          ]
        },
        "lat": {
          "anyOf": [
            {
              "$ref": "#/definitions/Decimal"
            },
            {
              "type": "null"
            }
          ]
        },
        "lng": {
          "anyOf": [
            {
              "$ref": "#/definitions/Decimal"
            },
            {
              "type": "null"
            }
          ]
        }
      }
    },
    "GeographicScope": {
      "description": "Encoded geographic scope rules of a Spec Version 2 `CarbonFootprint`",
      "anyOf": [
        {
          "type": "null"
        },
        {
          "type": "object",
          "required": [
            "geographyRegionOrSubregion"
          ],
          "properties": {
            "geographyRegionOrSubregion": {
              "$ref": "#/definitions/UNRegionOrSubregion"
            }
          }
        },
        {
          "type": "object",
          "required": [
            "geographyCountry"
          ],
          "properties": {
            "geographyCountry": {
              "$ref": "#/definitions/ISO3166CC"
            }
          }
        },
        {
          "type": "object",
          "required": [
            "geographyCountrySubdivision"
          ],
          "properties": {
            "geographyCountrySubdivision": {
              "$ref": "#/definitions/NonEmptyString"
            }
          }
        }
      ]
    },
    "UNRegionOrSubregion": {
      "description": "List of UN regions and subregions",
      "type": "string",
      "enum": [
        "Africa",
        "Americas",
        "Asia",
        "Europe",
        "Oceania",
        "Australia and New Zealand",
        "Central Asia",
        "Eastern Asia",
        "Eastern Europe",
        "Latin America and the Caribbean",
        "Melanesia",
        "Micronesia",
        "Northern Africa",
        "Northern America",
        "Northern Europe",
        "Polynesia",
        "South-eastern Asia",
        "Southern Asia",
        "Southern Europe",
        "Sub-Saharan Africa",
        "Western Asia",
        "Western Europe"
      ]
    },
    "ISO3166CC": {
      "type": "string"
    },
    "NonEmptyString": {
      "type": "string",
      "minLength": 1
    },
    "IataCode": {
      "type": "string"
    },
    "Locode": {
      "type": "string"
    },
    "UicCode": {
      "type": "string"
    },
    "Decimal": {
      "type": "string",
      "pattern": "^-?\\d+(\\.\\d+)?$"
    },
    "TransportMode": {
      "type": "string",
      "enum": [
        "Road",
        "Rail",
        "Air",
        "Sea",
        "InlandWaterway"
      ]
    },
    "PackagingOrTrEqType": {
      "type": "string",
      "enum": [
        "Box",
        "Pallet",
        "Container"
      ]
    },
    "NonEmptyVec_for_EnergyCarrier": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/EnergyCarrier"
      }
    },
    "EnergyCarrier": {
      "type": "object",
      "required": [
        "emissionFactorTTW",
        "emissionFactorWTW",
        "energyCarrier"
      ],
      "properties": {
        "energyCarrier": {
          "$ref": "#/definitions/EnergyCarrierType"
        },
        "feedstocks": {
          "type": [
            "array",
            "null"
          ],
          "items": {
            "$ref": "#/definitions/Feedstock"
          }
        },
        "energyConsumption": {
          "anyOf": [
            {
              "$ref": "#/definitions/Decimal"
            },
            {
              "type": "null"
            }
          ]
        },
        "energyConsumptionUnit": {
          "anyOf": [
            {
              "$ref": "#/definitions/EnergyConsumptionUnit"
            },
            {
              "type": "null"
            }
          ]
        },
        "emissionFactorWTW": {
          "$ref": "#/definitions/Decimal"
        },
        "emissionFactorTTW": {
          "$ref": "#/definitions/Decimal"
        }
      }
    },
    "EnergyCarrierType": {
      "type": "string",
      "enum": [
        "Diesel",
        "HVO",
        "Petrol",
        "CNG",
        "LNG",
        "LPG",
        "HFO",
        "MGO",
        "Aviation fuel",
        "Hydrogen",
        "Methanol",
        "Electric"
      ]
    },
    "Feedstock": {
      "type": "object",
      "required": [
        "feedstock"
      ],
      "properties": {
        "feedstock": {
          "$ref": "#/definitions/FeedstockType"
        },
        "feedstockPercentage": {
          "anyOf": [
            {
              "$ref": "#/definitions/Decimal"
            },
            {
              "type": "null"
            }
          ]
        },
        "regionProvenance": {
          "type": [
            "string",
            "null"
          ]
        }
      }
    },
    "FeedstockType": {
      "type": "string",
      "enum": [
        "Fossil",
        "Natural gas",
        "Grid",
        "Renewable electricity",
        "Cooking oil"
      ]
    },
    "EnergyConsumptionUnit": {
      "type": "string",
      "enum": [
        "l",
        "kg",
        "kWh",
        "MJ"
      ]
    },
    "HocCo2eIntensityThroughput": {
      "type": "string",
      "enum": [
        "TEU",
        "tonnes"
      ]
    },
    "PfId": {
      "type": "string",
      "format": "uuid"
    },
    "VersionString": {
      "type": "string",
      "minLength": 5,
      "pattern": "^\\d+\\.\\d+\\.\\d+(-\\d{8})?$"
    },
    "NonEmptyPfIdVec": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uuid"
      },
      "minItems": 1,
      "uniqueItems": true
    },
    "VersionInteger": {
      "type": "integer",
      "format": "int32",
      "maximum": 2147483647.0,
      "minimum": 0.0
    },
    "PfStatus": {
      "type": "string",
      "enum": [
        "Active",
        "Deprecated"
      ]
    },
    "CompanyIdSet": {
      "type": "array",
      "items": {
        "type": "string",
        "pattern": "^([uU][rR][nN]):"
      },
      "minItems": 1,
      "uniqueItems": true
    },
    "GenericURN": {
      "type": "string",
      "pattern": "^([uU][rR][nN]):"
    },
    "ProductIdSet": {
      "type": "array",
      "items": {
        "type": "string",
        "pattern": "^([uU][rR][nN]):"
      },
      "minItems": 1,
      "uniqueItems": true
    },
    "CarbonFootprint": {
      "description": "Data Type \"CarbonFootprint\" of Spec Version 2",
      "type": "object",
      "oneOf": [
        {
          "type": "object",
          "required": [
            "geographyRegionOrSubregion"
          ],
          "properties": {
            "geographyRegionOrSubregion": {
              "$ref": "#/definitions/UNRegionOrSubregion"
            },
            "geographyCountry": false,
            "geographyCountrySubdivision": false
          }
        },
        {
          "type": "object",
          "required": [
            "geographyCountry"
          ],
          "properties": {
            "geographyCountry": {
              "$ref": "#/definitions/ISO3166CC"
            },
            "geographyRegionOrSubregion": false,
            "geographyCountrySubdivision": false
          }
        },
        {
          "type": "object",
          "required": [
            "geographyCountrySubdivision"
          ],
          "properties": {
            "geographyCountrySubdivision": {
              "$ref": "#/definitions/NonEmptyString"
            },
            "geographyRegionOrSubregion": false,
            "geographyCountry": false
          }
        },
        {
          "type": "object",
          "properties": {
            "geographyRegionOrSubregion": false,
            "geographyCountry": false,
            "geographyCountrySubdivision": false
          }
        }
      ]
    },
    "DataModelExtension": {
      "type": "object",
      "required": [
        "data",
        "dataSchema",
        "specVersion"
      ],
      "properties": {
        "dataSchema": {
          "type": "string"
        },
        "documentation": {
          "type": "string"
        },
        "specVersion": {
          "$ref": "#/definitions/VersionString"
        },
        "data": {
          "$ref": "#/definitions/Hoc"
        }
      }
    }
  }
}